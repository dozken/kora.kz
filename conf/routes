# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET     /ad                           controllers.Application.createAd()

GET     /adlist                           controllers.Application.adList()

GET     /item                           controllers.Application.item()

GET     /profile                           controllers.Application.profile()

GET     /myMessages                           controllers.Application.myMessages()

GET     /myMoney                           controllers.Application.myMoney()

GET     /mySettings                           controllers.Application.mySettings()

GET     /myProfile                           controllers.Application.myProfile()

GET     /myProfileEdit                           controllers.Application.myProfileEdit()

GET     /adminPage                           controllers.Application.adminPage()

GET     /addFilter                           controllers.Application.addFilter()

GET     /addReklam                           controllers.Application.addReklam()

GET     /adminMessage                           controllers.Application.adminMessage()

GET     /adminProfile                           controllers.Application.adminProfile()

GET     /adminProfileEdit                           controllers.Application.adminProfileEdit()

GET     /users                           controllers.Application.users()

GET     /about                           controllers.Application.about()

GET     /read                           controllers.Application.read()

GET     /adminRead                           controllers.Application.adminRead()

GET     /feedback                           controllers.Application.feedback()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
GET     /webjars/*file                    controllers.WebJarAssets.at(file)
